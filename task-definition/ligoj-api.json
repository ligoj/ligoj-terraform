{
  "dnsSearchDomains": null,
  "environmentFiles": null,
  "logConfiguration": {
    "logDriver": "awslogs",
    "secretOptions": null,
    "options": {
      "awslogs-group": "/ecs/ligoj-api-${environment}",
      "awslogs-region": "${region}",
      "awslogs-stream-prefix": "ecs"
    }
  },
  "entryPoint": null,
  "portMappings": [],
  "command": null,
  "linuxParameters": null,
  "cpu": ${cpu},
  "environment": [
    {
      "name": "CRYPTO",
      "value": ""
    },
    {
      "name": "CUSTOM_OPTS",
      "value": " -Djdbc.host=${db_host} -Djdbc.user=${db_user} -Dligoj.plugin.install=${ligoj_plugins} -Dserver.forward-headers-strategy=NATIVE -XX:ActiveProcessorCount=${nb_cpu}"
    }
  ],
  "resourceRequirements": null,
  "ulimits": [
    {
       "name": "nofile",
       "softLimit": 2048,
       "hardLimit": 8192
    }
  ],
  "dnsServers": null,
  "mountPoints": [
    {
      "containerPath": "/home/ligoj",
      "sourceVolume": "efs"
    }
  ],
  "workingDirectory": null,
  "secrets": [
    {
      "name": "API_TOKEN_SECRET",
      "valueFrom": "${db_tdp_arn}"
    },
    {
      "name": "APP_CRYPTO_PASSWORD",
      "valueFrom": "${db_tdp_arn}"
    },
    {
      "name": "JDBC_PASSWORD",
      "valueFrom": "${db_password_arn}"
    }
  ],
  "dockerSecurityOptions": null,
  "memory": null,
  "memoryReservation": null,
  "volumesFrom": [],
  "stopTimeout": null,
  "image": "${repository}ligoj/ligoj-api:${ligoj_version}",
  "startTimeout": null,
  "firelensConfiguration": null,
  "dependsOn": null,
  "disableNetworking": null,
  "interactive": null,
  "healthCheck": {
    "retries": 3,
    "command": [
      "CMD-SHELL",
      "exit 0"
    ],
    "timeout": 5,
    "interval": 30,
    "startPeriod": null
  },
  "essential": true,
  "links": null,
  "hostname": null,
  "extraHosts": null,
  "pseudoTerminal": null,
  "user": null,
  "readonlyRootFilesystem": true,
  "dockerLabels": null,
  "systemControls": null,
  "privileged": null,
  "name": "ligoj-api"
}